display 4/3 read limit
assign x = 1
assign count = 0
while count < limit do
	display x
	assign count = count+1
	assign x = x*(x+1)
end
if count = limit
   then
	display count=limit
if (x < 3)
   then
	display -600
else
	display (x+1)*3
end
end
if (limit+5)
   then
	display limit
end